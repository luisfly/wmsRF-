@{
    ViewBag.Title = "拣货";
    Layout = "~/Views/Shared/_Layout.cshtml";
    string AssignTypeID = Request.QueryString["AssignTypeID"].ToString();
}
<!--AssignTypeID=2.托拣;3.箱拣;4.零拣;5.自动补货;6.手工移仓;7.全仓补货-->
<div  class="box">
<input id="AssignTypeID" type="hidden" value="@AssignTypeID" />
</div>
<div class="btn"><input  type="button" value="继续拣货(F8)" id="btnNext" onclick="Pick();" />  <input  type="button" value="集货(F9)" id="btnSet" onclick="javascript:location.href='/Set'"" /></div>
<script type="text/javascript">
    $(document).ready(function () {
        $(document).keydown(function (e) {
            var elem = e.srcElement || e.target;
            if (e.which == 119)//F8
            {
                $("#btnNext").click();
            }
            if (e.which == 120)//F9
            {
                $("#btnSet").click();
            }
        });

    });

    function Pick() {
        var AssignTypeID = $("#AssignTypeID").val();
        if (AssignTypeID == "0" || AssignTypeID == "4" || AssignTypeID == "3" || AssignTypeID == "2") {
            window.location.href = "/PICK?AssignTypeID=" + AssignTypeID;
        }
        else {
            window.location.href = "/Menu";
        }
    }
</script>